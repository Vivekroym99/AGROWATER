# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Earth Engine
GEE_SERVICE_ACCOUNT_EMAIL=your_gee_service_account@project.iam.gserviceaccount.com
GEE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_private_key_here\n-----END PRIVATE KEY-----"

# Resend Email
RESEND_API_KEY=re_your_resend_api_key

# App URL (for email links)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Security - Upstash Redis (optional, uses in-memory fallback if not set)
# Get these from https://console.upstash.com/
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_upstash_token

# Security - Cron job secret (required in production)
CRON_SECRET=your_random_secret_for_cron_jobs

# Web Push Notifications (VAPID)
# Generate with: node scripts/generate-vapid-keys.js
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your_vapid_public_key
VAPID_PRIVATE_KEY=your_vapid_private_key
VAPID_SUBJECT=mailto:admin@agrowater.pl

# Weather Integration (OpenWeatherMap)
# Get free API key from: https://openweathermap.org/api
OPENWEATHERMAP_API_KEY=your_openweathermap_api_key
